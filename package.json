{
  "name": "moove",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "yarn workspace @moove/foundry account",
    "account:generate": "yarn workspace @moove/foundry account:generate",
    "account:import": "yarn workspace @moove/foundry account:import",
    "chain": "yarn workspace @moove/foundry chain",
    "compile": "yarn workspace @moove/foundry compile",
    "deploy": "yarn workspace @moove/foundry deploy",
    "deploy:verify": "yarn workspace @moove/foundry deploy:verify",
    "flatten": "yarn workspace @moove/foundry flatten",
    "fork": "yarn workspace @moove/foundry fork",
    "format": "yarn next:format && yarn foundry:format",
    "foundry:format": "yarn workspace @moove/foundry format",
    "foundry:lint": "yarn workspace @moove/foundry lint",
    "foundry:test": "yarn workspace @moove/foundry test",
    "generate": "yarn account:generate",
    "postinstall": "husky install",
    "next:build": "yarn workspace @moove/nextjs build",
    "next:check-types": "yarn workspace @moove/nextjs check-types",
    "next:format": "yarn workspace @moove/nextjs format",
    "next:lint": "yarn workspace @moove/nextjs lint",
    "next:serve": "yarn workspace @moove/nextjs serve",
    "precommit": "lint-staged",
    "start": "yarn workspace @moove/nextjs dev",
    "test": "yarn foundry:test",
    "vercel": "yarn workspace @moove/nextjs vercel",
    "vercel:yolo": "yarn workspace @moove/nextjs vercel:yolo",
    "verify": "yarn workspace @moove/foundry verify"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.18.0"
  },
  "dependencies": {
    "react-icons": "^5.3.0",
    "styled-components": "^6.1.13"
  }
}
